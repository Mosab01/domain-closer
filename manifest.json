{
  "manifest_version": 3,
  "name": "Blocked Domain Checker",
  "version": "1.0",
  "permissions": ["contextMenus", "scripting", "tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "options.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
